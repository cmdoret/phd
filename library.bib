file = {:home/varogh/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Freeman, Aragon-Alcaide, Strunnikov - 2000 - The condensin complex governs chromosome condensation and mitotic transmission of rDNA(2).pdf:pdf},
