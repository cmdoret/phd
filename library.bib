@article{guoCRISPRInversionCTCF2015,
  title = {{{CRISPR Inversion}} of {{CTCF Sites Alters Genome Topology}} and {{Enhancer}}/{{Promoter Function}}},
  author = {Guo, Ya and Xu, Quan and Canzio, Daniele and Shou, Jia and Li, Jinhuan and Gorkin, David U. and Jung, Inkyung and Wu, Haiyang and Zhai, Yanan and Tang, Yuanxiao and Lu, Yichao and Wu, Yonghu and Jia, Zhilian and Li, Wei and Zhang, Michael Q. and Ren, Bing and Krainer, Adrian R. and Maniatis, Tom and Wu, Qiang},
  year = {2015},
