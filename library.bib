  author = {Du Bois, Ilona and Marsico, Annalisa and Bertrams, Wilhelm and Schweiger, Michal R. and Caffrey, Brian E. and {Sittka-Stark}, Alexandra and Eberhardt, Martin and Vera, Julio and Vingron, Martin and Schmeck, Bernd T.},
