@article{dietersdorferStarvedViableNonculturable2018,
  title = {Starved Viable but Non-Culturable ({{VBNC}}) {{Legionella}} Strains Can Infect and Replicate in Amoebae and Human Macrophages},
  author = {Dietersdorfer, Elisabeth and Kirschner, Alexander and Schrammel, Barbara and {Ohradanova-Repic}, Anna and Stockinger, Hannes and Sommer, Regina and Walochnik, Julia and {Cervero-Arag{\'o}}, S{\'i}lvia},
  year = {2018},
