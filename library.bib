file = {:home/varogh/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Zamboni et al. - 2006 - The Birc1e cytosolic pattern-recognition receptor contributes to the detection and control of Legionella pneumop.pdf:pdf},
