keywords = {Epigenomics,Genome organization,Hi-C,Malaria,Virulence},
number = {8},
pages = {3183--3192},
pmid = {30723152},
title = {{Comparative 3D genome organization in apicomplexan parasites}},
volume = {116},
year = {2019}
}
